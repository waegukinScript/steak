// ================ Start Header Area =================
header.header-area
  .container
    .header-wrap
      .header-top.d-flex.justify-content-between.align-items-center.navbar-expand-md
        .col.menu-left
          a(href='index.html') Home
          a(href='menu.html') menu
          a(href='about.html') about
        .col-3.logo
          a(href='index.html')
            img.mx-auto(src='img/logo.png' alt='')
        nav.col.navbar.navbar-expand-md.justify-content-end
          // Toggler/collapsibe Button
          button.navbar-toggler(type='button' data-toggle='collapse' data-target='#collapsibleNavbar')
            span.lnr.lnr-menu
          // Navbar links
          #collapsibleNavbar.collapse.navbar-collapse.menu-right
            ul.navbar-nav.justify-content-center.w-100
              li.nav-item.hide-lg
                a.nav-link(href='index.html') Home
              li.nav-item.hide-lg
                a.nav-link(href='menu.html') menu
              li.nav-item.hide-lg
                a.nav-link(href='about.html') about
              // Dropdown
              li.nav-item.dropdown
                a#navbardrop.nav-link.dropdown-toggle(href='#' data-toggle='dropdown')
                  | Pages
                .dropdown-menu
                  a.dropdown-item(href='contact.html') Contact
                  a.dropdown-item(href='elements.html') Elements
              li.nav-item.dropdown
                a#navbardrop.nav-link.dropdown-toggle(href='#' data-toggle='dropdown')
                  | Blog
                .dropdown-menu
                  a.dropdown-item(href='blog-home.html') Blog
                  a.dropdown-item(href='blog-single.html') Blog Detail
              li.nav-item
                a.nav-link.active(href='gallery.html') Gallery
// ================ End Header Area =================
// ================ Start banner Area =================
section.banner-area.relative
  .container
    .row.height.align-items-center.justify-content-center
      .banner-content.col-lg-6
        h1 image gallery
        hr
        .breadcrmb
          p
            a(href='index.html') home
            span.lnr.lnr-arrow-right
            a(href='gallery.html') gallery
// ================ End banner Area =================
