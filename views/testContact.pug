doctype html
html(lang='en')
  head
     title Jade
     style.
       table{
       border-collapse: collapse;
       margin-top: 30px;
       margin-left: 70px;
       margin-right: 30px;
       }
       .heading{
       font-weight: bold;
       width: 150px;
       }
       .value{
       width: 500px;
       }
   body
     div(style='margin-top: 50px')
       h1(style='margin-left: 70px;') Contacts
       hr(style=' margin-top:0px; height:10px;border:none;color:#333;background-color:#333; margin-left: 70px; margin-right: 73px;')
       each contact, index in contactsReceived
           p(style='margin-left: 70px;')= index+1
           table(border='1')
             tbody
                   tr
                     td.heading Contact Name
                     td.value= contact.contactName
                   tr
                     td.heading Email Address
                     td.value= contact.emailAddress
                   tr
                     td.heading Phone Number
                     td.value= contact.phoneNumber
                   tr
                     td.heading Post Message
                     td.value= contact.postMessage
                   tr
                     td.heading Date Entered
                     td.value= contact.createdAt

